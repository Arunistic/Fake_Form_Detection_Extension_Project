{
  "manifest_version": 3,
  "name": "Fake Form Detection (Enhanced)",
  "version": "1.0.0",
  "description": "Advanced phishing form detection with ML, Safe Browsing API, blacklist system, and JavaScript interception.",
  "permissions": [
    "storage",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "config.js",
        "utils.js",
        "blacklist.js",
        "ml-detector.js",
        "content_script.js"
      ],
      "run_at": "document_idle"
    }
  ]
}
